#include <stdio.h>
#include <stdlib.h>


int main() {
	// is icin katilma kurallari
	int yas,uni_ort;
	char tecrube[4],diploma[4];
	printf("sirketimize hosgeldin\n");
	printf("yasiniz kac\n");
	scanf("%d",&yas);
	if(yas>30)
	{
		printf("maalasef yasiniz standartlara uymiyor\n");
		return 0;
	}
	printf("tecrubeniz var mi (yes/no)");
	scanf("%s",&tecrube);
	if(strcmp(tecrube,"no")==0)
	{
		printf("tecrubeli bir elemeana ihtiyacimiz var\n" );
		return 0;
	}
	printf("diplomaniz varmi (yes/no)");
	scanf("%s",&diploma);
	if(strcmp(diploma,"no")==0)
	{
		printf("en az universite diplomasi lazim\n");
		return 0;
	}
	
    else 
    {
    	printf("ï¿½niversite ortalmaniz kac\n");
    	scanf("%d",&uni_ort);
	}
	printf("tesekurler is gorusmesi icin hazirsiniz\n");
	
	
	
	
	return 0;
}
